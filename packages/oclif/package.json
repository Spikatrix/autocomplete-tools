{
  "name": "@withfig/oclif",
  "description": "Generate Fig completion spec from oclif",
  "version": "1.0.2",
  "author": "Matt Schrage @mattschrage",
  "bugs": "https://github.com/withfig/autocomplete-tools/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3",
    "@types/node": "^10",
    "eslint": "^5.13",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/mattschrage/fig-oclif-plugin",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "publishConfig": {
    "access": "public"
  }
}
